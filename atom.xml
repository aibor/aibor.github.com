---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <author>
    <name>aiBo</name>
    <uri>http://blog.aibor.de/</uri>
  </author>
  <title>aiBo - blog</title>
  <link href="http://blog.aibor.de/atom.xml" rel="self" />
  <link href="http://blog.aibor.de/" />
  <id>http://blog.aibor.de/</id>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
{% for post in site.posts limit:17 %} 
  <entry>
    <author>
      <name>aiBo</name>
      <uri>http://blog.aibor.de/</uri>
    </author>
    <title>{{ post.title }}</title>
    <link href="http://blog.aibor.de{{ post.url }}" />
    <id>http://blog.aibor.de{{ post.id }}</id>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <content type="html">{{ post.content | xml_escape}}</content>
  </entry>
  {% endfor %}
</feed>
